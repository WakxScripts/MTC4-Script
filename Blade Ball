local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Main = OrionLib:MakeWindow({Name = "Blade Ball | Wakx", HidePremium = false, SaveConfig = false, ConfigFolder = "BladeBallWakx"})

local Players = game.Players
local Player = Players.LocalPlayer

local TanksFolder = workspace:WaitForChild("SpawnedVehicles")
local CharactersFolder = workspace:WaitForChild("SpawnedPlayers")

local Tab = Main:MakeTab({
	Name = "Main",
	Icon = "http://www.roblox.com/asset/?id=13060262529",
	PremiumOnly = false
})


Tab:AddTogle({
	Name = "This is a toggle!",
	Default = false,
	Callback = function(Value)
		print(Value)
	end    
})

OrionLib:Init()
